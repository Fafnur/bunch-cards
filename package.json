{
  "name": "bunch",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve web-cards",
    "start:api": "docker-compose -f ./apps/api-cards/docker-compose.yml up -d && nx serve api-cards",
    "stop:api": "docker-compose -f ./apps/api-cards/docker-compose.yml stop",
    "build": "nx build",
    "test": "nx test",
    "postinstall": "ngcc --properties es2020 browser module main",
    "dev:ssr": "nx run web-cards:serve-ssr",
    "serve:ssr": "node dist/web-cards/server/ru-RU/main.js",
    "build:ssr": "nx build web-cards && ng run web-cards:server",
    "prerender": "nx run web-cards:prerender",
    "extract-i18n": "nx extract-i18n web-cards --out-file=libs/web/localization/src/lib/russian/source.xlf",
    "generate-sitemap": "ts-node --project=apps/web-cards/tsconfig.sitemap.json apps/web-cards/sitemap-generator.ts",
    "typeorm": "ts-node -P apps/api-cards/tsconfig.app.json  -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config apps/api-cards/src/app/config/ormconfig.ts",
    "typeorm:migrate": "yarn typeorm-ts-node-esm migration:generate -d apps/api-cards/migrations",
    "typeorm:create": "yarn typeorm-ts-node-esm migration:create apps/api-cards/migrations/<NAME>",
    "typeorm:run": "yarn run typeorm migration:run -d apps/api-cards/migrations",
    "typeorm:revert": "yarn run typeorm migration:revert",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "prepare": "husky install"
  },
  "private": true,
